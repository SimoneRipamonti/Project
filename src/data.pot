[domain]
  #Domain is [0,domain_length]
  domain_length=1
  
  [../]

[physical_parameters]  
  #Permeability
  K_distr=1.0e-10+0*x
  #K_distr=1.0e-9*(x<0.5)+1.0e-10*(x>=0.5) # for testing code with different permeability
  #Water Viscosity
  mu=1.0e-3
  
  #Porosity
  #phi0=0.2+0*x
  phi0=1+0.0*x
  #phi0=0.0*x
  [../]

[BC_vel]

   #In-flow
   Q_in=0.
   
   #Out-flow
   Q_out=1.0e-1
   #Q_out=2.1e-1 #for testing code with different permeability

   #Pression on the left
   p_in=1.0e6
   
   #Pression on the righ
   p_out=0.0

   #Inflow cond
   in=Pressure #or Flow
   
   #Outflow cond
   out=Flow #or Pressure 
   
  [../]

[BC_trac]
    #Boundary cond for C
    
    #In-flow cond
    C_in=0.0

    #Out-flow cond
    C_out=0.0

   #Type of cond
    bc_trac=In

    [../]

[Source]
    #External Source
    #f=1.0e-1*(x>0.4)*(x<0.6) #da cambiare
    f=1.0e-1 #for testing code with different permeability
   [../]

[Discretization]
    #Cell number
    Nx=100

    [../]

[Time]
    #Final T
    T=1
    
    #Time step
    Nt=100
    
    [../]

[Initial_Condition]
    #Ca0 initial condition
    Ca0=1.0*(x<0.5)

    #caSiO3 initial condition
    CaSiO30=1.0*(x<0.5)
    
    [../]

[Reaction]
     #Temperature
     Temperature=1.0
     
     #Reaction Area
     Area_react=1.0

     #Constant_reaction
     rate_const=1.0

     #Energy_activation
     E=1.0

     #R constant
     R=1.0 
     
     #Ph
     ph=1.0
    
    [../]



     #Equilibrium constant
     K_eq=1.0

  
