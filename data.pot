[domain]
  #Domain is [0,domain_length]
  domain_length=1
  
  [../]

[physical_parameters]  
  #Permeability
  K_distr=1.0e-10+0*x
  
  #Water Viscosity
  mu=1.0e-3
  
  #Porosity
  #phi0=0.2+0*x
  phi0=1.0+0.0*(x<0.6)
  [../]

[BC_vel]

   #In-flow
   Q_in=0.
   
   #Out-flow
   Q_out=1.0e-1

   #Pression on the left
   p_in=1.0e6
   
   #Pression on the right
   p_out=0.

   #Inflow cond
   in=Pressure #or Flow
   
   #Outflow cond
   out=Flow #or Pressure 
   
  [../]

[BC_trac]
    #Boundary cond for C
    
    #In-flow cond
    C_in=1.0

    #Out-flow cond
    C_out=0.0

   #Type of cond
    bc_trac=In

    [../]

[Source]
    #External Source
    f=1.0e-1*x #da cambiare
    
   [../]

[Discretization]
    #Cell number
    Nx=10

    [../]

[Time]
    #Final T
    T=1
    #Time step
    Nt=10
    
    [../]

[Initial_Condition]
    #concentration initial condition
    IC=0.0*(x<0.5)
    
    
    
    
