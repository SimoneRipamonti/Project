[domain]
  #Domain is [0,domain_length]
  ###domain_length=500
  domain_length=0.45 #(45 mm)
  [../]

[physical_parameters]  
  #Permeability
  #K_distr=1.0e-10+0*x
  #K_distr=1.0e-9*(x<0.5)+1.0e-10*(x>=0.5) # for testing code with different permeability
  K_distr=2e-13+0*x
  #Water Viscosity
  #mu=1.0e-3
  mu=0.2e-3
  
  #Porosity
  #phi0=0.2+0*x
  phi0=0.3+0.0*x
  #phi0=1.0+0.0*x
  #phi0=0.0*x
  [../]

[BC_vel]

   #In-flow
   Q_in=0.
   
   #Out-flow
   Q_out=1.0e-1
   #Q_out=2.1e-1 #for testing code with different permeability

   #Pression on the left
   p_in=1.0e6
   
   #Pression on the righ
   p_out=0.0

   #Inflow cond
   in=Pressure #or Flow
   
   #Outflow cond
   out=Flow #or Pressure 
   
  [../]

[BC_trac]
    #Boundary cond for C
    
    #In-flow cond
    C_in=0.3691108
    #C_in=1.0
    #Out-flow cond
    C_out=0.0

   #Type of cond
    bc_trac=In

   #Numerical scheme
   method=Implicit #Implicit
 
   #Initial Condition
   #C_0=1.0*(x<0.5)
   #C_0=1.0*(x>0.05)*(x<0.2)
   C_0=0.0+0.0*x
    [../]

[Source]
    #External Source
    #f=1.0e-1*(x>0.4)*(x<0.6) #da cambiare
    #f=1.0e-1 #for testing code with different permeability
     f=0.0
    [../]

[Discretization]
    #Cell number
    Nx=100

    [../]

[Time]
    #Final T
    #Final T
    #T=100000
    T=345600
    #T=1.0
    #Time step
    Nt=100
    
    [../]


[Reaction]
     #Initical conentration Ca0
     Ca_0=0.0+0.0*x

     #Initical conentration H_piu
     H_piu_0=10.0e-3.5+0.0*x
     #H_piu_0=0.00031622777+0.0*x 
     #H_piu_0=0.0+0.0*x  
     #10.0e-3.5+0.0*x


     #Initical conentration HCO3_meno
     HCO3_meno_0=0.0+0.0*x

     #Initical conentration CO2
     ###CO2_0=1.0e-4+0.0*x
     CO2_0=0.3681108+0.0*x

     #Initical conentration CaSiO3
     CaSiO3_0=16.75041876+0.0*x
     #CaSiO3_0=16.75041876
     #CaSiO3_0=0.0+0.0*x
     #Initical conentration SiO2
     SiO2_0=0.0+0.0*x

     #Reaction_area
     #A=1.38e4 #1.38*10e-7
     A=0.0
     #Constant_reaction
     #Rate_const=10e{-1.3}
     Rate_const=0.05011872336
     
     #Energy_activation
     E=47000

     #R constant
     R=8.314
   
     #Temperature
     #Temp=423.0 #150 gradi
     Temp=423.0
     #K_sol
     K_sol=10e9#sarebbe 10^(9.012)

     #K_eq
     K_eq=4.45e-7 #un po di perplessitÃ  su questo valore

     #exponent for H_piu
     #n=0.31
     n=0.
     #Ph
     ph=-7.0

     #kp_i
     kp_i=3.98e-13
   
     #temporal scheme
     method=1

    [../]


  
