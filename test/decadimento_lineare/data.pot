[domain]
  #Domain is [0,domain_length]
  domain_length=1
  
  [../]

[physical_parameters]  
  #Permeability
  K_distr=1.0e-10+0*x
  #K_distr=1.0e-9*(x<0.5)+1.0e-10*(x>=0.5) # for testing code with different permeability
  #Water Viscosity
  mu=1.0e-3
  
  #Porosity
  #phi0=0.2+0*x
  phi0=1+0.0*x
  #phi0=0.0*x
  [../]

[BC_vel]

   #In-flow
   Q_in=0.
   
   #Out-flow
   Q_out=1.0e-1
   #Q_out=2.1e-1 #for testing code with different permeability

   #Pression on the left
   p_in=1.0e6
   
   #Pression on the righ
   p_out=0.0

   #Inflow cond
   in=Pressure #or Flow
   
   #Outflow cond
   out=Flow #or Pressure 
   
  [../]

[BC_trac]
    #Boundary cond for C
    
    #In-flow cond
    C_in=0.0

    #Out-flow cond
    C_out=0.0

   #Type of cond
    bc_trac=In

    [../]

[Source]
    #External Source
    #f=1.0e-1*(x>0.4)*(x<0.6) #da cambiare
    f=1.0e-1 #for testing code with different permeability
   [../]

[Discretization]
    #Cell number
    Nx=10

    [../]

[Time]
    #Final T
    T=1
    
    #Time step
    Nt=10
    
    [../]

[Initial_Condition]
    #Ca0 initial condition
    #Ca0=1.0*(x<0.5)
    Ca0=0.0+0.0*x
    #caSiO3 initial condition
    CaSiO30=1.0+0.0*x
    
    [../]

[Reaction]
     #Initical conentration Ca0
     Ca_0=1.0+0.0*x

     #Initical conentration H_piu
     H_piu_0=1.0e-7+0.0*x

     #Initical conentration HCO3_meno
     HCO3_meno_0=1.0+0.0*x

     #Initical conentration CO2
     CO2_0=1.0e-4+0.0*x

     #Initical conentration CaSiO3
     CaSiO3_0=1.0+0.0*x

     #Initical conentration SiO2
     SiO2_0=1.0+0.0*x

     #Reaction_area
     #A=1.38
     A=1

     #Constant_reaction
     #Rate_const=10e{-1.3}
     #Rate_const=0.05011872336
     Rate_const=1
     
     #Energy_activation
     #E=47000
     E=0
     
     #R constant
     #R=8.314
     R=1
   
     #Temperature
     #Temp=373.0
     Temp=1
   
     #K_sol
     #K_sol=10e9
     K_sol=1e3
     #K_sol=1

     #K_eq
     K_eq=1

     #exponent for H_piu
     n=0.0
     
     #Ph
     ph=0.0

     #Lambda for linear decay
     lambda=0.01
    
    [../]


  
